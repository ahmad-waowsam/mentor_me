<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"

    xmlns:app="http://schemas.android.com/apk/res-auto"

    tools:context=".SearchFragment">


    <TextView
        android:id="@+id/donthave"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="#157177"
        android:text="<-"
        android:textSize="30dp"

        android:layout_alignParentLeft="true"
        android:layout_marginLeft="24dp"
        android:layout_alignParentTop="true"
        android:layout_marginTop="34dp"
        android:textStyle="bold"



        />
    <ImageView
        android:id="@+id/circleTop"
        android:layout_width="200dp"
        android:layout_height="200dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true"
        android:layout_marginStart="-100dp"
        android:alpha="0.5"
        android:background="@drawable/bigcircle" />

    <!-- Second Circle (Touching Left) -->
    <ImageView
        android:id="@+id/circleLeft"
        android:layout_width="200dp"
        android:layout_height="150dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true"
        android:layout_marginTop="-70dp"
        android:alpha="0.5"
        android:background="@drawable/bigcircle" />

    <RelativeLayout
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        >

        <!-- res/layout/layout_search_bar.xml -->
        <TextView
            android:id="@+id/hellomsg"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="24dp"
            android:layout_marginTop="80dp"
            android:text="Search Results"
            android:textColor="#157177"

            android:textSize="30dp"
            android:textStyle="bold" />


        <Button
            android:layout_height="wrap_content"
            android:layout_width="wrap_content"
            android:layout_alignParentEnd="true"
            android:layout_marginRight="20dp"
            android:text="Filter"
            android:textAllCaps="false"
            android:backgroundTint="#157177"
            android:alpha="0.17"
            android:textColor="@color/white"
            android:layout_marginTop="150dp"

            />




        </RelativeLayout>




    <!-- res/layout/sample_layout.xml -->
    <LinearLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:layout_marginTop="240dp"
        android:padding="16dp">

        <!-- Square Rounded Circle -->
        <ImageView
            android:layout_width="80dp"
            android:layout_height="80dp"
            android:background="@drawable/square_rounded_circle"
            android:src="@drawable/square_rounded_circle"
        android:layout_gravity="center_vertical"/>

        <!-- Smaller Rectangular Shape with Information -->
        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:orientation="vertical"
            android:layout_marginStart="16dp">

            <!-- Name and Rate -->
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/nameTextView"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:text="Sample 1"
                    android:textStyle="bold"
                    android:textSize="18sp" />

                <TextView
                    android:id="@+id/rateTextView"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Rate" />

            </LinearLayout>

            <!-- Position and Heart Icon -->
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:layout_marginTop="8dp">

                <TextView
                    android:id="@+id/positionTextView"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:text="Lead - Tech officer" />

                <ImageView
                    android:layout_width="24dp"
                    android:layout_height="24dp"
                    android:src="@drawable/baseline_heart_broken_24" />

            </LinearLayout>

            <!-- Availability TextField -->
            <TextView
                android:id="@+id/availabilityTextView"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Available"
                android:textColor="#008000"
            android:layout_marginTop="8dp" />

        </LinearLayout>



    </LinearLayout>
